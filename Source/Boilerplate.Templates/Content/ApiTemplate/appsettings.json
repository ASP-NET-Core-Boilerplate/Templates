{
  //#if (ApplicationInsights)
  "ApplicationInsights": {
    // The instrumentation key for Application Insights retrieved from Azure e.g. 11111111-2222-3333-4444-555555555555
    "InstrumentationKey": "APPLICATION-INSIGHTS-INSTRUMENTATION-KEY"
  },
  //#endif
  "CacheProfiles": {
    // Cache static files for a year.
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "Compression": {
    // Additional MIME types (other than the built in defaults) to enable response compression for.
    "MimeTypes": [
      // e.g. ATOM
      // "application/atom+xml",
    ]
  },
  "HttpServer": {
    "Endpoints": {
      //#if (HttpsEverywhere)
      "Http": {
        "Host": "localhost",
        "Port": 5000,
        "Scheme": "http"
      },
      "Https": {
        "Host": "localhost",
        "Port": 5001,
        "Scheme": "https",
        "FilePath": "DevelopmentCertificate.pfx",
        "Password": "password"
      }
      //#else
      //"Http": {
      //  "Host": "localhost",
      //  "Port": 5000,
      //  "Scheme": "http"
      //}
      //#endif
    }
  },
  // Read more about configuring Serilog here https://github.com/serilog/serilog-settings-configuration
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentUserName",
      "WithExceptionDetails",
      "WithMachineName",
      "WithProcessId",
      "WithProcessName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Theme": "Code"
      },
      { "Name": "Debug" }
    ]
  }
}